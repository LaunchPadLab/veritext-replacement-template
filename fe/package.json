{
  "name": "@veritext-replacement-template/fe",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^18.12.1"
  },
  "scripts": {
    "analyze-bundle": "yarn && yarn generate-bundle-stats && yarn bundle-analyzer-server",
    "build:development": "webpack serve --config ./config/webpack.config.development.js",
    "build": "webpack --config ./config/webpack.config.production.js",
    "bundle-analyzer-server": "webpack-bundle-analyzer .webpack-stats.json build/",
    "format": "prettier --write \"src/**/*.+(js|jsx|json)\"",
    "generate-bundle-stats": "webpack --config config/webpack.config.production.js --profile --json > .webpack-stats.json",
    "lint": "eslint src/ --fix --max-warnings=0",
    "seed:test": "echo 'No script specified, seed skipped'",
    "start": "yarn && yarn build:development",
    "test:integration": "cypress run",
    "test:unit": "jest",
    "test": "yarn && node scripts/test-suite.mjs",
    "server": "yarn --cwd ../be node server.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@launchpadlab/babel-preset": "^2.1.1",
    "@launchpadlab/lp-components": "^8.0.0",
    "@launchpadlab/lp-form": "^2.10.1",
    "@launchpadlab/lp-redux-api": "^6.2.0",
    "@launchpadlab/lp-redux-utils": "^1.3.0",
    "@launchpadlab/lp-requests": "^4.2.2",
    "@sentry/browser": "^7.26.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^9.1.0",
    "bourbon": "^7.2.0",
    "bourbon-neat": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.9.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.19.2",
    "css-loader": "^6.7.3",
    "dot-index-webpack-plugin": "^5.2.0",
    "expose-env-middleware": "^2.0.0",
    "express": "^4.17.3",
    "express-enforces-ssl": "^1.1.0",
    "figaro-js": "^3.0.2",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^2.0.4",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^7.0.2",
    "prop-types": "^15.8.1",
    "prop-types-exact": "^1.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-flash": "^2.0.2",
    "redux-form": "^8.3.8",
    "redux-sessions": "^2.0.2",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.0.0",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@launchpadlab/eslint-config": "^2.7.0",
    "@launchpadlab/lp-subsection-generator": "^7.2.1",
    "@launchpadlab/prettier-config": "^1.0.0",
    "chalk": "^5.2.0",
    "cli-prompt": "^0.6.0",
    "cypress": "^12.1.0",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react-dev-utils": "^12.0.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ],
    "src/**/*.{json,scss}": "prettier --write"
  }
}